#!nsh
#
# @name CruiseAder Claire VTAIL
#
# @type VTOL Tiltrotor
# @class VTOL
#
# @maintainer Samay Siga <samay_s@icloud.com>
#
# 
#

sh /etc/init.d/rc.vtol_defaults

if [ $AUTOCNF == yes ]
then
param set VT_TYPE 1
param set VT_TILT_MC 0.08
param set VT_TILT_TRANS 0.5
param set VT_TILT_FW 0.9

param set VT_MOT_COUNT 4
param set VT_FW_MOT_OFFID 24
param set VT_IDLE_PWM_MC 1100
param set VT_TYPE 1

param set FW_AIRSPD_MIN 14
param set FW_AIRSPD_TRIM 20
param set FW_AIRSPD_MAX 26

param set FW_MAN_P_MAX 55
param set FW_MAN_R_MAX 55
param set FW_R_LIM 55

param set FW_WR_FF 0.2
param set FW_WR_I 0.2
param set FW_WR_IMAX 0.8
param set FW_WR_P 1
param set FW_W_RMAX 0
fi

set MIXER claire
set PWM_OUT 1234
set PWM_RATE 400
set PWM_MAX 2000

set MIXER_AUX vtol_AAVV
set PWM_AUX_RATE 50
set PWM_AUX_MIN 1000
set PWM_AUX_MAX 2000
set PWM_AUX_DISARMED 1000

set MAV_TYPE 21
